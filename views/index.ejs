<!DOCTYPE html>
<html>
  <head>
    <title>User</title>
    <link rel="stylesheet" href="/stylesheets/style.css" />
  </head>
  <body>
    <h1>Danh sách người dùng</h1>
    <a href="/form-add"><button>Thêm người dùng</button></a> <br />
    <table border="1">
      <tr>
        <th>ID</th>
        <th>Phone</th>
        <th>Password</th>
        <th>Type Register</th>
        <th>Token Login</th>
        <th>Device ID</th>
        <th>Fcm Tokens</th>
        <th>ACTION</th>
      </tr>
      <% users.forEach(item => { %>
      <tr>
        <td><%= item._id%></td>
        <td><%= item.phone%></td>
        <td><%= item.password%></td>
        <td><%= item.typeRegister%></td>
        <td><%= item.tokenLogin%></td>
        <td><%= item.deviceID%></td>
        <td><%= item.fcmTokens%></td>
        <td>
          <a href="/form-update/<%= item._id%>">Update</a>||<a
            href="/form-delete/<%= item._id%>"
            >Delete</a
          >
        </td>
      </tr>
      <% }) %>
    </table>
  </body>
</html>
